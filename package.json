{
  "name": "ntust-auto-repeal-votes",
  "version": "0.1.0",
  "description": "",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run dev:vite\" \"bun run dev:inject\"",
    "dev:vite": "vite dev",
    "dev:inject": "rollup -c rollup.config.inject.js -w",
    "build": "bun run build:inject&& bun run build:vite",
    "build:vite": "vite build",
    "build:inject": "rollup -c rollup.config.inject.js",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "tauri": "tauri"
  },
  "license": "MIT",
  "dependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-shell": "~2",
    "lucide-svelte": "^0.464.0",
    "svelte-radix": "^2.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@sveltejs/adapter-static": "^3.0.5",
    "@sveltejs/kit": "^2.7.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tauri-apps/cli": "^2",
    "autoprefixer": "^10.4.20",
    "bits-ui": "^0.21.16",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.0",
    "rollup": "^4.28.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwind-merge": "^2.5.5",
    "tailwind-variants": "^0.3.0",
    "tailwindcss": "^3.4.9",
    "tslib": "^2.8.0",
    "typescript": "^5.5.0",
    "vite": "^5.4.10"
  }
}
